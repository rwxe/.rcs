{
	"coc.preferences.jumpCommand":"drop",
	"coc.preferences.rootPatterns": [
		".git",
		".env",
		"env",
		".venv",
		"venv",
		"setup.cfg",
		"pyvenv.cfg",
		"setup.py",
		".projections.json"
	],
    "python.pythonPath": "python",
	"sql.database": "mysql",
	"sql.formatOptions": {
		"language":"mysql",
		"indent":"    ",
		"linesBetweenQueries":2,
		"uppercase":true
	},
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".git/"],
      "filetypes": ["go"]
    }
  }
}
